{
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/step2",
      "dest": "/step2.html"
    },
    {
      "src": "/completion",
      "dest": "/completion.html"
    },
    {
      "src": "/dashboard",
      "dest": "/dashboard.html"
    }
  ],
  "headers": [
    {
      "source": "/fonts/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}