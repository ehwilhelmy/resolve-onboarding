{
  "rewrites": [
    {
      "source": "/step2",
      "destination": "/step2.html"
    },
    {
      "source": "/completion",
      "destination": "/completion.html"
    },
    {
      "source": "/dashboard",
      "destination": "/dashboard.html"
    }
  ],
  "headers": [
    {
      "source": "/fonts/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}